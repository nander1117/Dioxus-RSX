{
  "name": "dioxus-rsx",
  "displayName": "Dioxus RSX Syntax Highlighter",
  "description": "Provides syntax highlighting for Dioxus RSX files.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "publisher": "Jhon AF",
  "icon": "assets/icons/icon.ico",
  "categories": [
    "Programming Languages"
  ],
  "repository": {
      "type": "git",
      "url": "https://github.com/nander1117/Dioxus-RSX"
  },
  "bugs": {
        "url": "https://github.com/nander1117/Dioxus-RSX/issues"
  },
  "homepage": "https://github.com/nander1117/Dioxus-RSX/README.md",
  "contributes": {

    "grammars": [{
      "injectTo": ["source.rust"],
      "patterns": [
        {
          "begin": "rsx!\\s*\\{",
          "end": "\\}",
          "contentName": "text.html.rsx"
        }
      ],
      "scopeName": "source.dioxus.rsx",
      "path": "./syntaxes/dioxus-rsx.tmLanguage.json"
    }]
  }
}
